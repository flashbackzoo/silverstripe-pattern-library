{
    "name": "flashbackzoo/silverstripe-pattern-library",
    "description": "Framework agnostic pattern library generator for Silverstripe",
    "homepage": "https://github.com/flashbackzoo/silverstripe-pattern-library",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "pattern library",
        "storybook"
    ],
    "authors": [
        {
            "name": "David Craig",
            "homepage": "https://davidcraig.co.nz",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/flashbackzoo/silverstripe-pattern-library/issues"
    },
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0 || ^5.0",
        "silverstripe/admin": "^1.0 || ^2.0",
        "symfony/yaml": "^4.4.0 || ^5 || ^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Flashbackzoo\\SilverstripePatternLibrary\\": "src/",
            "Flashbackzoo\\SilverstripePatternLibrary\\Tests\\": "tests/"
        }
    },
    "extra": {
        "installer-name": "silverstripe-pattern-library",
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
